{
    "manifest_version":3,
    "version":"1.0",
    "description": "Still use Bookmarks?? Save your tab link just in one click. ",
    "name":"My Link",
    "action":{
        "default_popup":"index.html",
        "default_icon":"images/icons/my-link128.png"
    },
    "icons":{
        "128":"images/icons/my-link128.png"
    },
    "permissions":[
        "tabs"
    ]
}